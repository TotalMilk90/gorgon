    fj_vertex_1     = 1067 + (2074 << 16)
    fj_vertex_2     = 979 + (2150 << 16)
    fj_vertex_3     = 1196 + (2363 << 16)
    STR_VAR
    fj_structure_type   = region
    fj_name             = EnterGorgon
    fj_destination_area = h_guilda.are
    fj_destination_name = InsideGorgon
  END

